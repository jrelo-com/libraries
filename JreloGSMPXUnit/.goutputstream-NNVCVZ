#pragma once
#include <Arduino.h>
#include <Module.h>
#include <SIMZ.h>

class OutgoingCallModule : public Module {

    private :
    
		SIMZ *simz = NULL;
		
    public :

        OutgoingCallModule(const char *key, SIMZ *simz);
        ~OutgoingCallModule();
        void update();
        void inputData(const char *data);
        void outputData(Pipe *pipe);
		void call(const char *phoneNumber);
        
};
